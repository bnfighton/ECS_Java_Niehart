
public class YourSchedule 
{
	public static void main( String[] args)
	{
		String teacher1, class1, teacher2, class2, teacher3, class3, teacher4, class4;
		
		teacher1 = "Thow";
		class1 = "English";
		teacher2 = "Mauro";
		class2 = "ECS";
		teacher3 = "Ancona";
		class3 = "Spanish";
		teacher4 = "Peck";
		class4 = "Math";
		
		System.out.println("------------------------------------------");
		System.out.println("");
		System.out.println("| Per 1  |    " + class1 + "    |     " + teacher1 +"     |");
		System.out.println("| Per 2  |");
		System.out.println("| Per 3  |");
		System.out.println("| Per 4  |");
		System.out.println("");
		System.out.println("------------------------------------------");
		
		
	}

}
